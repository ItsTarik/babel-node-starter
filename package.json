{
  "name": "babel-node-starter",
  "version": "1.0.0",
  "description": "starter for node.js using ES with",
  "main": "src/index.js",
  "scripts": {
    "dev": "npx nodemon --exec npx babel-node src/index.js",
    "build": "babel src -d dist",
